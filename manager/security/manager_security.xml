<?xml version="1.0" ?>
<odoo>
    <record id="module_manager_category" model="ir.module.category">
        <field name="name">Quan Ly Doanh Nghiep</field>
    </record>
    <!-- Library User Group -->
    <record id="manager_group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_manager_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

     <!-- Library Manager Group -->
    <record id="manager_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_manager_category"/>
        <field name="implied_ids" eval="[(4, ref('manager_group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')),(4, ref('base.user_admin'))]"/>
    </record>
